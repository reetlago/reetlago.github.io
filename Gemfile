source 'https://rubygems.org'

# Blog generator
# https://github.com/jekyll/jekyll
gem 'jekyll'

group :jekyll_plugins do
  # CoffeeScript
  # https://github.com/jekyll/jekyll-coffeescript
  gem 'jekyll-coffeescript'

  # Sitemap generator
  # https://github.com/jekyll/jekyll-sitemap
  gem 'jekyll-sitemap'
end
